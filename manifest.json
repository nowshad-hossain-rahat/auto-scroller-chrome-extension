{
    "manifest_version": 2,
    "name": "NHR Auto Scroller",
    "description": "This extension will automatically scroll up or down after page load!",
    "version": "1.0.0",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["main.js"]
        }
    ]
}